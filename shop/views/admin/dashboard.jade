extends layout
block content
    section#main-content
        section.wrapper
            .row
                .col-lg-9.main-chart
                    .row.mtbox
                        .col-md-2.col-sm-2.col-md-offset-1.box0
                            .box1
                                span.li_heart
                                h3 933
                            p 933 People liked your page the last 24hs. Whoohoo!
                        .col-md-2.col-sm-2.box0
                            .box1
                                span.li_cloud
                                h3 +48
                            p 48 New files were added in your cloud storage.
                        .col-md-2.col-sm-2.box0
                            .box1
                                span.li_stack
                                h3 23
                            p You have 23 unread messages in your inbox.
                        .col-md-2.col-sm-2.box0
                            .box1
                                span.li_news
                                h3 +10
                            p More than 10 news were added in your reader.
                        .col-md-2.col-sm-2.box0
                            .box1
                                span.li_data
                                h3 OK!
                            p Your server is working perfectly. Relax & enjoy.
                    // /row mt
                    .row.mt
                        // SERVER STATUS PANELS
                        .col-md-4.col-sm-4.mb
                            .white-panel.pn.donut-chart
                                .white-header
                                    h5 SERVER LOAD
                                .row
                                    .col-sm-6.col-xs-6.goleft
                                        p
                                            i.fa.fa-database
                                            | 70%
                                canvas#serverstatus01(height='120', width='120')
                                script.
                                  var doughnutData = [
                                  {
                                  value: 70,
                                  color:"#68dff0"
                                  },
                                  {
                                  value : 30,
                                  color : "#fdfdfd"
                                  }
                                  ];
                                  var myDoughnut = new Chart(document.getElementById("serverstatus01").getContext("2d")).Doughnut(doughnutData);
                        // /col-md-4
                        .col-md-4.col-sm-4.mb
                            .white-panel.pn
                                .white-header
                                    h5 TOP PRODUCT
                                .row
                                    .col-sm-6.col-xs-6.goleft
                                        p
                                            i.fa.fa-heart
                                            | 122
                                    .col-sm-6.col-xs-6
                                .centered
                                    img(src='assets/img/product.png', width='120')
                        // /col-md-4
                        .col-md-4.mb
                            // WHITE PANEL - TOP USER
                            .white-panel.pn
                                .white-header
                                    h5 TOP USER
                                p
                                    img.img-circle(src='assets/img/ui-zac.jpg', width='80')
                                p
                                    b Zac Snider
                                .row
                                    .col-md-6
                                        p.small.mt MEMBER SINCE
                                        p 2012
                                    .col-md-6
                                        p.small.mt TOTAL SPEND
                                        p $ 47,60
                        // /col-md-4
                    // /row
                    .row
                        // TWITTER PANEL
                        .col-md-4.mb
                            .darkblue-panel.pn
                                .darkblue-header
                                    h5 DROPBOX STATICS
                                canvas#serverstatus02(height='120', width='120')
                                script.
                                  var doughnutData = [
                                  {
                                  value: 60,
                                  color:"#68dff0"
                                  },
                                  {
                                  value : 40,
                                  color : "#444c57"
                                  }
                                  ];
                                  var myDoughnut = new Chart(document.getElementById("serverstatus02").getContext("2d")).Doughnut(doughnutData);
                                p April 17, 2014
                                footer
                                    .pull-left
                                        h5
                                            i.fa.fa-hdd-o
                                            | 17 GB
                                    .pull-right
                                        h5 60% Used
                        // /col-md-4
                        .col-md-4.mb
                            // INSTAGRAM PANEL
                            .instagram-panel.pn
                                i.fa.fa-instagram.fa-4x
                                p
                                    | @THISISYOU
                                    br
                                    | 5 min. ago
                                p
                                    i.fa.fa-comment
                                    | 18
                                    i.fa.fa-heart
                                    | 49
                        // /col-md-4
                        .col-md-4.col-sm-4.mb
                            // REVENUE PANEL
                            .darkblue-panel.pn
                                .darkblue-header
                                    h5 REVENUE
                                .chart.mt
                                    .sparkline(data-type='line', data-resize='true', data-height='75', data-width='90%', data-line-width='1', data-line-color='#fff', data-spot-color='#fff', data-fill-color='', data-highlight-line-color='#fff', data-spot-radius='4', data-data='[200,135,667,333,526,996,564,123,890,464,655]')
                                p.mt
                                    b $ 17,980
                                    br
                                    | Month Income
                        // /col-md-4
                    // /row
                    .row.mt
                        // CUSTOM CHART START
                        .border-head
                            h3 VISITS
                        .custom-bar-chart
                            ul.y-axis
                                li
                                    span 10.000
                                li
                                    span 8.000
                                li
                                    span 6.000
                                li
                                    span 4.000
                                li
                                    span 2.000
                                li
                                    span 0
                            .bar
                                .title JAN
                                .value.tooltips(data-original-title='8.500', data-toggle='tooltip', data-placement='top') 85%
                            .bar
                                .title FEB
                                .value.tooltips(data-original-title='5.000', data-toggle='tooltip', data-placement='top') 50%
                            .bar
                                .title MAR
                                .value.tooltips(data-original-title='6.000', data-toggle='tooltip', data-placement='top') 60%
                            .bar
                                .title APR
                                .value.tooltips(data-original-title='4.500', data-toggle='tooltip', data-placement='top') 45%
                            .bar
                                .title MAY
                                .value.tooltips(data-original-title='3.200', data-toggle='tooltip', data-placement='top') 32%
                            .bar
                                .title JUN
                                .value.tooltips(data-original-title='6.200', data-toggle='tooltip', data-placement='top') 62%
                            .bar
                                .title JUL
                                .value.tooltips(data-original-title='7.500', data-toggle='tooltip', data-placement='top') 75%
                        // custom chart end
                    // /row
                // /col-lg-9 END SECTION MIDDLE
                //
                  **********************************************************************************************************************************************************
                  RIGHT SIDEBAR CONTENT
                  ***********************************************************************************************************************************************************
                .col-lg-3.ds
                    // COMPLETED ACTIONS DONUTS CHART
                    h3 NOTIFICATIONS
                    // First Action
                    .desc
                        .thumb
                            span.badge.bg-theme
                                i.fa.fa-clock-o
                        .details
                            p
                                muted 2 Minutes Ago
                                br
                                a(href='#') James Brown
                                | subscribed to your newsletter.
                                br
                    // Second Action
                    .desc
                        .thumb
                            span.badge.bg-theme
                                i.fa.fa-clock-o
                        .details
                            p
                                muted 3 Hours Ago
                                br
                                a(href='#') Diana Kennedy
                                | purchased a year subscription.
                                br
                    // Third Action
                    .desc
                        .thumb
                            span.badge.bg-theme
                                i.fa.fa-clock-o
                        .details
                            p
                                muted 7 Hours Ago
                                br
                                a(href='#') Brandon Page
                                | purchased a year subscription.
                                br
                    // Fourth Action
                    .desc
                        .thumb
                            span.badge.bg-theme
                                i.fa.fa-clock-o
                        .details
                            p
                                muted 11 Hours Ago
                                br
                                a(href='#') Mark Twain
                                | commented your post.
                                br
                    // Fifth Action
                    .desc
                        .thumb
                            span.badge.bg-theme
                                i.fa.fa-clock-o
                        .details
                            p
                                muted 18 Hours Ago
                                br
                                a(href='#') Daniel Pratt
                                | purchased a wallet in your store.
                                br
                    // USERS ONLINE SECTION
                    h3 TEAM MEMBERS
                    // First Member
                    .desc
                        .thumb
                            img.img-circle(src='assets/img/ui-divya.jpg', width='35px', height='35px', align='')
                        .details
                            p
                                a(href='#') DIVYA MANIAN
                                br
                                muted Available
                    // Second Member
                    .desc
                        .thumb
                            img.img-circle(src='assets/img/ui-sherman.jpg', width='35px', height='35px', align='')
                        .details
                            p
                                a(href='#') DJ SHERMAN
                                br
                                muted I am Busy
                    // Third Member
                    .desc
                        .thumb
                            img.img-circle(src='assets/img/ui-danro.jpg', width='35px', height='35px', align='')
                        .details
                            p
                                a(href='#') DAN ROGERS
                                br
                                muted Available
                    // Fourth Member
                    .desc
                        .thumb
                            img.img-circle(src='assets/img/ui-zac.jpg', width='35px', height='35px', align='')
                        .details
                            p
                                a(href='#') Zac Sniders
                                br
                                muted Available
                    // Fifth Member
                    .desc
                        .thumb
                            img.img-circle(src='assets/img/ui-sam.jpg', width='35px', height='35px', align='')
                        .details
                            p
                                a(href='#') Marcel Newman
                                br
                                muted Available
                    // CALENDAR
                    #calendar.mb
                        .panel.green-panel.no-margin
                            .panel-body
                                #date-popover.popover.top(style='cursor: pointer; disadding: block; margin-left: 33%; margin-top: -50px; width: 175px;')
                                    .arrow
                                    h3.popover-title(style='disadding: none;')
                                    #date-popover-content.popover-content
                                #my-calendar
                    // / calendar
                // /col-lg-3
block scripts
