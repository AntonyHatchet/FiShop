                                i.fa.fa-tasks
                                span Forms
                            ul.sub
                                li
                                    a(href='form_component.html') Form Components
                        li.sub-menu
                            a(href='javascript:;')
                                i.fa.fa-th
                                span Data Tables
                            ul.sub
                                li
                                    a(href='basic_table.html') Basic Table
                                li
                                    a(href='responsive_table.html') Responsive Table
                        li.sub-menu
                            a(href='javascript:;')
                                i.fa.fa-bar-chart-o
                                span Charts
                            ul.sub
                                li
                                    a(href='morris.html') Morris
                                li
                                    a(href='chartjs.html') Chartjs
                    // sidebar menu end
            // sidebar end
